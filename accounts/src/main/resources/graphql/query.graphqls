type Query {
    accounts: [BankAccount]
}

type BankAccount {
    id: ID
    client: Client
    currency: Currency
    balance: Float
    status: String
}

type Client {
    id: ID
    person: PersonInfo
}

type PersonInfo{
    firstName: String
    middleName: String
    lastName: String
}

# Supported currencies
enum Currency {
    #Euro
    EUR,
    # Canadian Dollar
    CAD,
    # United States Dollar
    USD
}